{
    "entry": "./src/index.ts",
    "output": {
        "filename": "bundle.js",
        "path": "C:\\Data\\GitHub\\ts-css-loader\\test\\.test\\basic\\.output"
    },
    "resolve": {
        "extensions": [
            ".ts",
            ".tsx",
            ".js"
        ]
    },
    "module": {
        "rules": [
            {
                "test": {},
                "loader": "css-loader",
                "options": {
                    "modules": true,
                    "camelCase": true,
                    "localIdentName": "[path][name]-[local]"
                }
            },
            {
                "test": {},
                "use": [
                    {
                        "loader": "ts-loader",
                        "options": {
                            "usePreviousLoaderGeneratedFiles": true,
                            "silent": true
                        }
                    },
                    {
                        "loader": "ts-css-loader",
                        "options": {
                            "test": {}
                        }
                    }
                ]
            },
            {
                "test": {},
                "loader": "newLine"
            }
        ]
    },
    "context": "C:\\Data\\GitHub\\ts-css-loader\\test\\.test\\basic",
    "resolveLoader": {
        "alias": {
            "ts-css-loader": "C:\\Data\\GitHub\\ts-css-loader\\lib\\ts-css-loader.js",
            "passthrough-loader": "C:\\Data\\GitHub\\ts-css-loader\\lib\\passthrough-loader.js",
            "newLine": "C:\\Data\\GitHub\\ts-css-loader\\test\\lib\\newline.loader.js"
        }
    }
}