{
  "name": "ts-css-loader",
  "version": "1.0.0",
  "description": "Generate typescript typings for css modules loaded using css-loader",
  "main": "lib/loader.js",
  "scripts": {
    "pretest": "npm run clean-typings",
    "test": "webpack --config test/webpack.config.js --hide-modules",
    "prebuild-test": "rimraf test/lib/*",
    "build-test": "tsc -p test/src",
    "build-and-test": "npm run build && npm test",
    "clean-typings": "rimraf test/src/**/*.css.d.ts",
    "clean": "rimraf lib/* test/lib/* test/wwwroot/*",
    "prebuild": "npm run clean",
    "build": "tsc -p src",
    "prewatch": "npm run clean",
    "watch": "tsc -p src --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MortenHoustonLudvigsen/ts-css-loader.git"
  },
  "keywords": [
    "ts-loader",
    "webpack",
    "webpack loader",
    "loader",
    "typescript",
    "ts",
    "css",
    "TypeScript"
  ],
  "author": "Morten Houston Ludvigsen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MortenHoustonLudvigsen/ts-css-loader/issues"
  },
  "homepage": "https://github.com/MortenHoustonLudvigsen/ts-css-loader#readme",
  "devDependencies": {
    "@types/webpack": "^2.2.14",
    "css-loader": "^0.28.0",
    "rimraf": "^2.6.1",
    "ts-loader": "^2.0.3",
    "typescript": "^2.3.0",
    "webpack": "^2.3.3"
  },
  "dependencies": {
    "loader-utils": "^1.1.0"
  }
}
